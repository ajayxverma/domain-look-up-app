{
  "name": "backend",
  "type": "module",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "build": "rimraf dist && tsc",
    "start": "npx tsc && nodemon dist/server.js"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "node-cache": "^5.1.2",
    "pre-commit": "^1.2.2",
    "rimraf": "^6.0.1",
    "winston": "^3.8.2"
  },
  "pre-commit": [
    "ts.check",
    "build",
    "add-build"
  ],
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2"
  }
}
